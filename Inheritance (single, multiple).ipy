        "multiple inheritance"
      ],
      "metadata": {
        "id": "LqPIJK6_0tol"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "class father:\n",
        "    def father_info(self):\n",
        "        print(\"This is father node\")\n",
        "class mother:\n",
        "    def mother_info(self):\n",
        "        print(\"this is mother node\")\n",
        "class child(father,mother):\n",
        "    def child_info(self):\n",
        "        print(\"this is child node\")\n",
        "obj=child()\n",
        "obj.father_info()\n",
        "obj.mother_info()\n",
        "obj.child_info()"
      ],
