        "Single inheritance"
      ],
      "metadata": {
        "id": "b3jswvUf03Ui"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "class parent:\n",
        "    def display(self):\n",
        "        print(\"this is parent node\")\n",
        "class child(parent):\n",
        "    def show(self):\n",
        "        print(\"this is child node\")\n",
        "a=child()\n",
        "a.display()\n",
        "a.show()"
      ],
